class pattern7 
{
	public static void main(String[] args) 
	{  for(int r=1;r<=5;r++){
		for(int c=1;c<=5;c++){
		if(r==1 || c==3 || (r==5 && c<=3))
		System.out.print(" * ");
		else
			System.out.print("   ");
		
		}
		
		
		for(int c=6;c<=14;c++){
			if(r+c==11 || c-r==9 || (r==3 && c>=9 &&c<=12))
				System.out.print(" * ");
		else
			System.out.print("   ");
		}
		
		
		for(int c=15 ;c<=23;c++)
		{
			if(c-r==14 || r+c==24)
				System.out.print(" * ");
		else
			System.out.print("   ");
		}
		
		
		for(int c=24;c<=32;c++){
			if(r+c==29|| c-r==27 || (r==3 && c>=26 &&c<=30))
				System.out.print(" * ");
		else
			System.out.print("   ");
		}
		System.out.println();
	}
	
}
}



